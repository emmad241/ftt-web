<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/stylesheets/style.css" rel="stylesheet" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title><%= pageTitle %></title>
</head>
<body class="loggedin-body">
    <%- include('../partials/header_loggedin.ejs'); %>
    <div class="clients-table">
        <div class="headings-row">
            <p>Username</p>
            <p>Name</p>
            <p>Number</p>
            <p>Stocks</p>
            <p>Crypto</p>
        </div>
        <% clients.forEach(client => { %>
        <a class="clients-data" href="">
            <div class="clients-row">
                <div class="client-username"><%= client.username %></div>
                <div class="client-name"><%= client.name %></div>
                <div class="client-number"><%= client.number %></div>
                <div class="client-stocks"><%= client.stocks %></div>
                <div class="client-crypto"><%= client.crypto %></div>
            </div>
        </a>
        <% }); %>
    </div>
</body>
</html>